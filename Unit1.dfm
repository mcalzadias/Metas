  object QrMetas: TMSQuery
    SQLUpdate.Strings = (
      'UPDATE cx2_metas'
      'SET'
      
        '  Usuario = :Usuario, MetaUO = :MetaUO, Tienda = :Tienda, Nombre' +
        ' = :Nombre, MetaUE = :MetaUE, MetaUEHD = :MetaUEHD, MetaLL = :Me' +
        'taLL, MetaT = :MetaT, MetaC = :MetaC, MetaI = :MetaI, MetaS = :M' +
        'etaS, MetaB = :MetaB, MetaPT = :MetaPT, MetaPL = :MetaPL, Factur' +
        'a = :Factura, Devolucion = :Devolucion, MetaQ = :MetaQ, MetaQHD ' +
        '= :MetaQHD'
      'WHERE'
      '  indice = :Old_indice')
    Connection = Genesis
    SQL.Strings = (
      'select'
      
        'Usuario, isnull(MetaUO,0) as MetaUO, Tienda, Nombre, isnull(Meta' +
        'UE,0) as MetaUE, isnull(MetaUEHD,0) as MetaUEHD, '
      
        'isnull(MetaLL,0) as MetaLL, isnull(MetaT,0) as MetaT, isnull(Met' +
        'aC,0) as MetaC, isnull(MetaI,0) as MetaI, '
      
        'isnull(MetaS,0) as MetaS, isnull(MetaB,0) as MetaB, isnull(MetaP' +
        'T,0) as MetaPT, isnull(MetaPL,0) as MetaPL, Factura, Devolucion,' +
        ' '
      
        'isnull(MetaQ,0) as MetaQ, isnull(MetaQHD,0) as MetaQHD,isnull(Me' +
        'taTotal,0) as MetaTotal'
      ''
      'from cx2_metas where tienda=:tienda'
      'order by Nombre')
    Left = 681
    Top = 370
    ParamData = <
      item
        DataType = ftString
        Name = 'tienda'
      end>
  end
